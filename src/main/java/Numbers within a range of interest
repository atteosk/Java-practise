// You can create a list of numbers and print out the numbers from the certain range

import java.util.ArrayList;
import java.util.Scanner;

public class LuvutRajatullaAlueella {

    public static void main(String[] args) {
        Scanner lukija = new Scanner(System.in);

        ArrayList<Integer> luvut = new ArrayList<>();
        while (true) {
            int number = Integer.valueOf(lukija.nextLine());
            int i = number;
            i++;
        if (number == -1) {
                break;
             } 

              luvut.add(number);

          }
        
        System.out.println("Start point?");
        int minimi = Integer.valueOf(lukija.nextLine());
        System.out.println("Finish point?");
        int maksimi = Integer.valueOf(lukija.nextLine());

        while (minimi <= maksimi) {
          System.out.println(luvut.get(minimi));
          minimi++;
            

          }

        }
        
    }
