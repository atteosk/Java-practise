
import java.util.Scanner;

public class Nelilaskin {

    public static void main(String[] args) {
        Scanner lukija = new Scanner(System.in);
        
        System.out.println("Type in the first number");
        
        int eka = Integer.valueOf(lukija.nextLine());
        
        System.out.println("Type in the second number");
        
        int toka = Integer.valueOf(lukija.nextLine());
        
        int summa = eka + toka;
        
        int erotus = eka - toka; 
        
        int kerto = eka * toka; 
        
        double jako = (double) eka / toka;
        
        System.out.println(eka + " + " + toka + " = " + summa);
        
        System.out.println(eka + " - " + toka + " = " + erotus);
        
        System.out.println(eka + " * " + toka + " = " + kerto);
        
        System.out.println(eka + " / " + toka + " = " + jako);





        // toteuta ohjelma t√§nne

    }
}
